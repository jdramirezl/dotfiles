
function fdar() {
    python /Users/julianramire/dotfiles/Naboo/main.py "task" "run" "$@"
}

function cds(){
    python /Users/julianramire/dotfiles/Naboo/main.py "task-image" "check" "$@"
}

function fdap(){
    python /Users/julianramire/dotfiles/Naboo/main.py "task-image" "prepare" "$@"
}

function crs(){
    python /Users/julianramire/dotfiles/Naboo/main.py "task" "check" "$@"
}

alias crs="crs"
alias cds="cds"
alias fdap="fdap"
alias fdar="fdar"
