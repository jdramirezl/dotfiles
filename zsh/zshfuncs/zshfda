
function fdar() {
    python /Users/julianramire/dotfiles/fda_prepare_funcs.py "task" "run" "$@"
}

function cds(){
    python /Users/julianramire/dotfiles/fda_prepare_funcs.py "task-image" "check" "$@"
}

function fdap(){
    python /Users/julianramire/dotfiles/fda_prepare_funcs.py "task-image" "prepare" "$@"
}

alias crs="check_run_status"
alias cds="cds"
alias fdap="fdap"
alias fdar="fdar"
